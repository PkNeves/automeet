{
    "manifest_version": 3,
    "name": "AutoMeet",
    "version": "0.1",
    "icons": {
        "128": "/images/128.png"
    },
    "background": {
        "service_worker": "content.js"
    },
    "permissions": ["alarms", "tabs", "scripting", "storage"],
    "action": {
        "default_popup": "popup.html"
    }
}